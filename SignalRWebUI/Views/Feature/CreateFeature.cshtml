@{
	ViewData["Title"] = "Create Feature";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}
@using SignalRWebUI.Dtos.FeatureDtos
@model CreateFeatureDto

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Create Feature</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">

					<div class="card">
						<div class="card-header">
							<div class="card-title">Add New Feature</div>
						</div>
						<div class="card-body">

							<div class="form-group">
								<label for="Title1">Enter  Title1</label>
								<input type="text" class="form-control" id="Title1" asp-for="Title1">
							</div>
							<div class="form-group">
								<label for="Description1">Enter Description1</label>
								<textarea rows="3" class="form-control" id="Description1" asp-for="Description1"></textarea>
							</div>
							<div class="form-group">
								<label for="Title2">Enter  Title2</label>
								<input type="text" class="form-control" id="Title2" asp-for="Title2">
							</div>
							<div class="form-group">
								<label for="Description2">Enter Description2</label>
								<textarea rows="3" class="form-control" id="Description2" asp-for="Description2"></textarea>
							</div>
							<div class="form-group">
								<label for="Title3">Enter  Title3</label>
								<input type="text" class="form-control" id="Title3" asp-for="Title3">
							</div>
							<div class="form-group">
								<label for="Description3">Enter Description3</label>
								<textarea rows="3" class="form-control" id="Description3" asp-for="Description3"></textarea>
							</div>


						</div>
						<div class="card-action">
							<button class="btn btn-primary">Add Feature</button>

						</div>
					</div>

				</form>
			</div>
		</div>
	</div>
</div>

